let reverse = (arr) => {
    for (let i =0; i <arr.length; i++) {
        let ele = arr.pop();
        arr.slice(i, 0, ele)
    }
    retrun arr
}
console.logh (reverse([1, 2, 3]))

4
let lastEle = (arr) => {
    return arr[arr.length -1]
}
console.log(lastEle([1, 2, 3]))

5
let findIdx = (arr, val) => {
    return arr.indexof(val)
    for (let i =0; i < arr.length; i++) {
        if (arr[i] === val) {
            return i
        }
    }
}
console.log(findIdx([1, 2, 3], 1))

6. let concat = (arr1, arr2) => {
    return [ ...arr1, ...arr2,]

}
console.log(concat([1, 3, 5], [2, 6, 8]))

