       title: 'Walter Isaacson',
       author: 'Steve Jobs',
       libraryID: 4264
   },
   {
       title: 'Mockingjay: The Final Book of The Hunger Games',
       author: 'Suzanne Collins',
       libraryID: 3245
   }];
let a =Object.values({
    title: 'the road Ahead',
    author: 'Bill Gates',
    libraryID: 1254
})
console.log(a)
let sortArr = (arr) => {
    return arr.map(ele=> object.keys(ele).sort())
}
console.log(sortArr(library))

