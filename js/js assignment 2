var library = [ 
   {
       author: 'Bill Gates',
       title: 'The Road Ahead',
       readingStatus: true
   },
   {
       author: 'Steve Jobs',
       title: 'Walter Isaacson',
       readingStatus: true
   },
   {
       author: 'Suzanne Collins',
       title:  'Mockingjay: The Final Book of The Hunger Games', 
       readingStatus: false
   }];
   let rS = (obj) => {
       for(let i = 0; i < obj.length; i++) {
           console.log(obj[i])
           if (obj[i].readingStatus) {
               console.log(`Already read ${obj[i].author} by The ${obj[i].title}.`)
           }
       }
   }
   rS(library)

